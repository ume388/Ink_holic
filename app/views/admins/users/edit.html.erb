<div>
  <div>
    <h1><%= @user.user_name %>さんを編集する</h1>
    
    <div>
      <%= form_with model:@user, url: admins_user_path do |f| %>
        <div>プロフィール画像</div>
        <%= f.attachment_field :user_image %>
      
        <div>ユーザー名</div>
        <%= f.text_field :user_name %>
      
        <div>メールアドレス</div>
        <%= f.text_field :email %>
        
        <div>退会ステータス</div>
        <%= f.select :is_deleted, [["有効", "false"], ["退会", "true"]] %>
      
        <div>
        <%= f.submit "編集内容を保存" %>
        </div>
      <% end %>
    </div>
  </div>
</div>